{
    "version":2,
    "builds": 
        [{"src":"*.js", "use": "@vercel/node"}],
    "headers": [
        {
        "source": "/api/(.*)",
        "headers": [
            { "key": "Access-Control-Allow-Origin", "value": "https://manobal.vercel.app" },
            { "key": "Access-Control-Allow-Methods", "value": "GET,POST,OPTIONS" },
            { "key": "Access-Control-Allow-Headers", "value": "Content-Type" }
        ]
        }
    ]    
}